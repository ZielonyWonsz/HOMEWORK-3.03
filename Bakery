package Zad2;

public class Bakery extends Shop
{
    private String[] products = {"Kawa", "Herbata", "Energetyk", "Piwo 0%", "Ciastka", "Chipsy", "Żelki", "Czekolada"};
    Bakery(String address, String size)
    {
        super(address, size);
    }
    public void get_info()
    {
        super.get_info();
        System.out.println("Napoje zimne, ciepłe i jedzenie słodkie, słone");
        for (String i : products)
        {
            System.out.println(i);
        }
    }
}
